# JuneERP Production Environment

# Database Configuration
DATABASE_URL=postgresql://username:password@production-host:5432/juneerp

# Application Configuration
NODE_ENV=production
PORT=5000
VITE_PORT=5173

# Security Configuration
SESSION_SECRET=CHANGE-THIS-TO-A-SECURE-RANDOM-STRING

# Logging Configuration
LOG_LEVEL=warn

# Feature Flags
ENABLE_AI_FEATURES=true
ENABLE_ANALYTICS=true
ENABLE_DEBUG=false

# Production Security
SSL_REDIRECT=true
SECURE_COOKIES=true
CORS_ORIGIN=https://yourdomain.com
